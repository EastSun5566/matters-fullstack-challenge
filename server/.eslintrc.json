{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["airbnb-base", "plugin:import/typescript"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "import/extensions": "off"
  }
}
